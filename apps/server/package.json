    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3"
